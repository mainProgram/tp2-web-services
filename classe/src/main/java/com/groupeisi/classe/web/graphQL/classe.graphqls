type Query {
    getAllClasses: [Classe]
    getClasseById(id: ID!): Classe
}

type Mutation {
    createClasse(classeInput: ClasseInput!): Classe
    updateClasse(id: ID!, classeInput: ClasseInput!): Classe
    deleteClasse(id: ID!): Boolean
}

type Classe {
    id: ID
    libelle: String
    status: Boolean
    professeursIds: [ID]
}

input ClasseInput {
    libelle: String
    status: Boolean
    professeursIds: [ID]
}
